{
  "buildCommand": "yarn build",
  "outputDirectory": "dist",
  "devCommand": "yarn dev",
  "installCommand": "yarn install",
  "framework": "nextjs",
  "rewrites": [
    { "source": "/admin/:path*", "destination": "/api/[...payload]" },
    { "source": "/api/:path*", "destination": "/api/[...payload]" }
  ]
}